<odoo>

    <data noupdate="1">

        <record id="ir_cron_outsourced_qid_reminder" model="ir.cron">
              <field name="name">Outsourced QID Reminder</field>
              <field name="model_id" ref="model_hr_employee"/>
              <field name="state">code</field>
              <field name="code">model.outsourced_qid_reminder()</field>
              <field name="interval_number">1</field>
              <field name="interval_type">weeks</field>
              <field name="numbercall">-1</field>
              <field eval="False" name="doall"/>
       </record>

        <record id="ir_cron_outsourced_national_address_reminder" model="ir.cron">
              <field name="name">Outsourced National Address Reminder</field>
              <field name="model_id" ref="model_hr_employee"/>
              <field name="state">code</field>
              <field name="code">model.outsourced_national_address_reminder()</field>
              <field name="interval_number">1</field>
              <field name="interval_type">weeks</field>
              <field name="numbercall">-1</field>
              <field eval="False" name="doall"/>
       </record>

    </data>

</odoo>