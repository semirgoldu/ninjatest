<odoo>
    <data>
        <!--list view -->
        <record id="ebs_hr_eos_other_entitlements_types_tree" model="ir.ui.view">
            <field name="name">ebs.hr.eos.other.entitlements.types.tree</field>
            <field name="model">ebs.hr.eos.other.entitlements.types</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
<!--                    <field name="account_id"/>-->
                </tree>
            </field>
        </record>

        <record id="ebs_hr_eos_other_entitlements_types_form" model="ir.ui.view">
            <field name="name">ebs.hr.eos.other.entitlements.types.form</field>
            <field name="model">ebs.hr.eos.other.entitlements.types</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
<!--                            <field name="account_id"/>-->
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <!--window -->
        <record id="ebs_hr_eos_other_entitlements_types_action" model="ir.actions.act_window">
            <field name="name">EOS Other Entitlement Types</field>
            <field name="res_model">ebs.hr.eos.other.entitlements.types</field>
            <field name="view_mode">tree,form</field>
        </record>


        <!--menu -->


    </data>
</odoo>