<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record model="ir.sequence" id="notice_sequence">
            <field name="name">Notice Sequence</field>
            <field name="code">hr.notice</field>
            <field name="implementation">standard</field>
            <field name="active">True</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
        </record>

        <record model="ir.sequence" id="fusion_sequence">
            <field name="name">Main Company Employee Sequence</field>
            <field name="code">hr.fusion</field>
            <field name="prefix">FGH/</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field eval="False" name="company_id"/>
        </record>

        <record model="ir.sequence" id="client_sequence">
            <field name="name">Client Employee Sequence</field>
            <field name="code">hr.client</field>
            <field name="prefix">CLIENT/</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field eval="False" name="company_id"/>
        </record>

        <record model="ir.sequence" id="fos_sequence">
            <field name="name">FOS Employee Sequence</field>
            <field name="code">hr.fos</field>
            <field name="prefix">FOS/</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field eval="False" name="company_id"/>
        </record>
    </data>
</odoo>