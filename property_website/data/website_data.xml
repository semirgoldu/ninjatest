<odoo>
    <record id="website.menu_home" model="website.menu">
        <field name="name">Home</field>
        <field name="url">/</field>
        <field name="parent_id" ref="website.main_menu"/>
        <field name="sequence" type="int">10</field>
    </record>
    <record id="menu_website_rent" model="website.menu">
        <field name="name">Rent</field>
        <field name="url">/rent</field>
        <field name="parent_id" ref="website.main_menu"/>
        <field name="sequence" type="int">55</field>
    </record>
    <record id="menu_website_sale" model="website.menu">
        <field name="name">Sale</field>
        <field name="url">/sell</field>
        <field name="parent_id" ref="website.main_menu" />
        <field name="sequence" type="int">55</field>
    </record>

</odoo>
