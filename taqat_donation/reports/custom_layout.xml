        <template id="compuspeed_external_layout_standard">
            <div t-attf-class="o_company_#{company.id}_layout header row" t-att-style="report_header_style">
                <div class="is-subtotal text-left col-md-6 w-50">
                    <div name="td_section_subtotal">
                        <div>
                            <t t-if="company.partner_id.name">
                                <t style="font-weight:bold;" t-esc="company.partner_id.name"/>
                            </t>
                        </div>
                        <div>
                            <t t-if="company.street">
                                <t t-esc="company.street"/>
                            </t>
                            <t t-if="company.street2">
                                <t t-esc="company.street2"/>,
                            </t>
                        </div>
                        <div>
                            <t t-if="company.city">
                                <t t-esc="company.city"/>,
                            </t>
                            <t t-if="company.country_id.name">
                                <t t-esc="company.country_id.name"/>
                            </t>
                        </div>
                            <div>
                                <t t-if="o.company_id.phone">
                                    Voice:
                                    <t t-esc="o.company_id.phone"/>
                                </t>
                            </div>
                        <div>
                            <t t-if="o.company_id.partner_id.fax">
                                Fax:
                                <t t-esc="o.company_id.partner_id.fax"/>
                            </t>
                        </div>
                        <div>
                            <t t-if="o.company_id.email">
                                Email:
                                <t t-esc="o.company_id.email"/>
                            </t>
                        </div>
                        <div>
<!--                            <t t-if="company.p_o_box">-->
<!--                                P.O.Box:-->
<!--                                <t t-esc="company.p_o_box"/>-->
<!--                            </t>-->
                        </div>
                    </div>
                </div>
<!--                <div class="col-md-6 w-50">-->
<!--                    <img src="/ebs_mse_custom_account/static/src/img/MSE_Consulting_DMCC.png"-->
<!--                         style="max-width:200px;max-heights:100px; position:relative;" class="float-right"-->
<!--                         alt="Logo"/>-->
<!--                </div>-->
                <div class="col-md-6 w-50">
                    <div class="col-12">
                        <div class="col-12">
                            <img t-if="o.company_id.logo" t-att-src="image_data_uri(o.company_id.logo)"
                                 class="float-left" style="width:100%" alt="Logo"/>
                        </div>


                        <div class="col-12">
                            <t t-if="o.company_id.partner_id.vat">
                                VAT Reg. No:
                                <t t-esc="o.company_id.partner_id.vat"/>
                            </t>
                        </div>

                        <div class="col-12">
                            <t t-if="o.company_id.company_registry">
                                Business Reg. No:
                                <t t-esc="o.company_id.company_registry"/>
                            </t>
                        </div>

                    </div>
                </div>
            </div>
            <div t-attf-class="article o_report_layout_standard o_company_#{company.id}_layout"
                 t-att-data-oe-model="o and o._name" t-att-data-oe-id="o and o.id"
                 t-att-data-oe-lang="o and o.env.context.get('lang')">
                <div>
                    <t t-call="web.address_layout"/>
                </div>
                <t t-raw="0"/>
            </div>
        </template>
        <template id="compuspeed_quotation_custom_layout">
            <t t-if="not o" t-set="o" t-value="doc"/>
            <t t-if="not company">
                <!-- Multicompany -->
                <t t-if="company_id">
                    <t t-set="company" t-value="company_id"/>
                </t>
                <t t-elif="o and 'company_id' in o">
                    <t t-set="company" t-value="o.company_id.sudo()"/>
                </t>
                <t t-else="else">
                    <t t-set="company" t-value="res_company"/>
                </t>
            </t>
            <t t-call="compuspeed_report.compuspeed_external_layout_standard">
                <t t-raw="0"/>
            </t>
        </template>
