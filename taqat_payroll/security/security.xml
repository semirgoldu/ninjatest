<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">
        <record id="ir_rule_fos_employee" model="ir.rule">
            <field name="name">Payslip Batch:Outsourced Employee </field>
            <field name="model_id" ref="hr_payroll.model_hr_payslip_run"/>
            <field name="domain_force">[('employee_type','=','fos_employee')]</field>
            <field name="groups" eval="[(4, ref('ebs_lb_payroll.group_fos_payroll'))]"/>
        </record>
        <record id="ir_rule_main_company_employee" model="ir.rule">
            <field name="name">Payslip Batch:Main Company Employee </field>
            <field name="model_id" ref="hr_payroll.model_hr_payslip_run"/>
            <field name="domain_force">[('employee_type','=','fusion_employee')]</field>
            <field name="groups" eval="[(4, ref('ebs_lb_payroll.group_fusion_payroll'))]"/>
        </record>
    </data>
</odoo>