<odoo>
    <data noupdate="0">
        <record id="check_expiration_properties" model="ir.cron">
            <field name="name">Check Properties Expiration</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="nextcall" eval="(DateTime.now().replace(hour=0))"/>
            <field name="numbercall">-1</field>
            <field name="doall">True</field>
            <field name="active">True</field>
            <field name="model_id" ref="taqat_property_management.model_account_analytic_account"/>
            <field name="code" eval="'model.check_expiration_properties()'"/>
        </record>
        <record id="check_maturity_date" model="ir.cron">
            <field name="name">Check Maturity Date</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="nextcall" eval="(DateTime.now().replace(hour=0))"/>
            <field name="numbercall">-1</field>
            <field name="doall">True</field>
            <field name="active">True</field>
            <field name="model_id" ref="taqat_property_management.model_tenancy_rent_schedule"/>
            <field name="code" eval="'model.check_maturity_date()'"/>
        </record>
    </data>
</odoo>