<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="paperformat_taqat_tenancy_contract" model="report.paperformat">
            <field name="name">Tenancy Contract A4</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">5</field>
            <field name="margin_bottom">10</field>
            <field name="margin_left">10</field>
            <field name="margin_right">10</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">35</field>
            <field name="dpi">90</field>
        </record>

    <record id="action_report_tenancy_contract" model="ir.actions.report">
        <field name="name">Tenancy Contract</field>
        <field name="model">account.analytic.account</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">taqat_property_management.tenancy_contract</field>
        <field name="report_file">taqat_property_management.tenancy_contract</field>
        <field name="print_report_name">'Tenancy Contract'</field>
        <field name="binding_model_id" ref="property_management.model_account_analytic_account"/>
        <field name="paperformat_id" ref="taqat_property_management.paperformat_taqat_tenancy_contract"/>
        <field name="binding_type">report</field>
    </record>
</odoo>