<?xml version="1.0" encoding="utf-8"?>
<odoo>
		
        <record id="property_duplicate_views" model="ir.ui.view">
          <field name="name">property.duplicate.form</field>
          <field name="model">property.duplicate</field>
          <field name="arch" type="xml">
            <form string="Property Duplicate" >
              <group>
                  <group>
                <field name="number_of_duplicate"/>
                  </group>
                </group>
                <footer>
                    <button name="duplicate" string="Duplicate" type="object" class="oe_highlight"/>
                    <button string="Cancel" class="oe_link" special="cancel" />
                </footer>
            </form>
          </field>
       </record>

 
       <record id="action_property_duplicate" model="ir.actions.act_window">
          <field name="name">Property Duplicate</field>
          <field name="type">ir.actions.act_window</field>
          <field name="res_model">property.duplicate</field>
          <field name="view_mode">form</field>
          <field name="target">new</field>
       </record>
 </odoo>
