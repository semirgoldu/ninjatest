<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="taqat_hr_employee_form_view" model="ir.ui.view">
        <field name="name">taqat.hr.employee.form.view</field>
        <field name="model">hr.employee</field>
        <field name="priority" eval="60"/>
        <field name="inherit_id" ref="ebs_fusion_hr_employee.ebs_custom_hr_employee_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='sequence']" position="before">
                <field name="emp_code"/>
            </xpath>
        </field>
    </record>
</odoo>