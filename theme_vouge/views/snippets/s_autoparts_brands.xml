<?xml version="1.0" encoding="UTF-8"?>
<odoo>

	<template id="s_autoparts_brands" name="Our Brands">
		<section class="autoparts-brands pt32 pb32">
			<div class="container">
				<div class="heading text-center mb32">
					<span>Top Quality Partner</span>
					<h3 class="title">Shop by Brands</h3>
				</div>
				<div id="autopart-brand-carousel" class="carousel bs-slider slide  control-round indicators-line" data-ride="carousel" data-pause="hover" data-interval="7000">
