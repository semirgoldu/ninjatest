<?xml version="1.0" encoding="UTF-8"?>
<odoo>

	<template id="s_medical_banner" name="Home Banner">
		<section class="business-medical-home-banner">
			<div id="business-medical-carousel" class="carousel bs-slider slide  control-round indicators-line" data-ride="carousel" data-pause="hover" data-interval="7000">
	          	<div class="carousel-inner" role="listbox">
	            	<div class="carousel-item active" style="background-image: url('/theme_vouge/static/src/img/medical-banner2.jpg');">
	            		<div class="container-fluid">
	            			<div class="row position-relative">
	            				<div class="col-md-8 col-8">
	            					<div class="banner-content text-center">
	            						<span>Safety First</span>
	            						<h1 class="title1">Medical N95<br></br> Mask</h1>
	            						<div class="s_btn" data-name="Button">
	            							<a href="#" class="btn btn-medical bizople_button">Shop Now</a>
	            						</div>
	            					</div>
	            				</div>
	            			</div>
	            		</div>
	            	</div>
	            	<div class="carousel-item" style="background-image: url('/theme_vouge/static/src/img/medical-banner3.jpg');">
	            		<div class="container-fluid">
	            			<div class="row position-relative">
	            				<div class="col-md-8 col-8 offset-md-4">
	            					<div class="banner-content text-center">
	            						<span>Home Equipments</span>
	            						<h1 class="title1">Equipments For Anykind<br></br> of Emergency</h1>
	            						<div class="s_btn" data-name="Button">
	            							<a href="#" class="btn btn-medical bizople_button">View Collection</a>
	            						</div>
	            					</div>
	            				</div>
	            			</div>
	            		</div>
	            	</div>
	            	<div class="carousel-item" style="background-image: url('/theme_vouge/static/src/img/medical-banner1.jpg');">
	            		<div class="container-fluid">
	            			<div class="row position-relative justify-content-center">
	            				<div class="col-md-8 col-8">
	            					<div class="banner-content text-center">
	            						<span>Medical Equipment's</span>
	            						<h1 class="title1 text-dark">For Professional Doctor<br></br>&amp; Physician</h1>
	            						<div class="s_btn" data-name="Button">
	            							<a href="#" class="btn btn-medical bizople_button">View Products</a>
	            						</div>
	            					</div>
	            				</div>
	            			</div>
	            		</div>
	            	</div>
	          	</div>
		        <div class="slider-control">
              		<a class="left carousel-control-prev text-white font-14" data-target="#business-medical-carousel" role="button" data-slide="prev">
	                	<span class="fa fa-angle-left"/>
	               	 	<b class="sr-only font-normal o_default_snippet_text">Previous</b>
	              	</a>
	              	<a class="right carousel-control-next text-white font-14" data-target="#business-medical-carousel" role="button" data-slide="next">
	                	<span class="fa fa-angle-right"/>
	                	<b class="sr-only font-normal o_default_snippet_text">Next</b>
	              	</a>
	            </div>
        	</div>
		</section>
	</template>

</odoo>