<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="vouge_demo_template" model="ir.ui.view">
		<field name="name">Theme Vouge Demo</field>
		<field name="type">qweb</field>
		<field name="key">website.vouge_demo_template</field>
		<field name="arch" type="xml">
			<t name="Theme Vouge Demo" t-name="theme_vouge.vouge_demo_template">
				<t t-call="website.layout">
					<div class="oe_structure oe_empty" id="wrap">
						<section class="demo-banner " style="background-image: url('/theme_vouge/static/src/img/demo/demo-banner-bg.jpg');">
							<div class="container-fluid">
								<div class="row">
									<div class="col-md-6 col-lg-5">
										<div class="bg-image-marque h-100 shadow" style="background-image: url('/theme_vouge/static/src/img/demo/demo-banner.png');"></div>
									</div>
									<div class="col-md-6 col-lg-5 offset-lg-1 pt256 pb256">
										<div class="content">
											<h1 class="mb-5">Vouge Ecommerce, Multipurpose &amp; Responsive Odoo Theme</h1>
											<a class="btn btn-primary bizople_button_1" href="#"> Explore Demo </a>
											<div class="number-text d-flex mt-5">
												<div class="demos">
													<h2>29</h2>
													<p class="m-0">Pre Built Homepages</p>
												</div>
												<div class="demos">
													<h2>10</h2>
													<p class="m-0">Headers Styles</p>
												</div>
												<div class="demos">
													<h2>8</h2>
													<p class="m-0">Footers Styles</p>
												</div>
												<div class="demos">
													<h2>240+</h2>
													<p class="m-0">Awesome Snippets</p>
												</div>
											</div>
										</div>
									</div>
								</div>
							</div>
						</section>
					</div>
				</t>
			</t>
		</field>
	</record>
	<record id="vouge_demo_template_menu" model="website.page">
		<field name="website_published">True</field>
		<field name="url">/vouge-demo</field>
		<field name="view_id" ref="vouge_demo_template" />
	</record>
</odoo>